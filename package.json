{
  "name": "rdf-native-loader",
  "version": "0.0.0",
  "description": "Registry for JavaScript native types loader based on RDF descriptions",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zazuko/rdf-native-loader.git"
  },
  "keywords": [],
  "author": "Thomas Bergwinkl <bergi@axolotlfarm.org> (https://www.bergnet.org/people/bergi/card#me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zazuko/rdf-native-loader/issues"
  },
  "homepage": "https://github.com/zazuko/rdf-native-loader",
  "dependencies": {
    "@rdfjs/data-model": "^1.1.1"
  },
  "devDependencies": {
    "clownface": "^0.10.0",
    "expect": "^24.1.0",
    "jest": "^24.1.0",
    "rdf-ext": "^1.2.2",
    "standard": "^12.0.1",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.0",
    "husky": "^1.0.0",
    "lint-staged": "^8.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "standard-version": "^6.0.1"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "*.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
